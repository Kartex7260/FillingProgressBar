<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:progress="1.0" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar2"
        app:layout_constraintTop_toTopOf="parent"
        app:progress="0.75" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar3"
        app:layout_constraintTop_toTopOf="parent"
        app:progress="0.5" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar4"
        app:layout_constraintTop_toTopOf="parent"
        app:progress="0.25" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar5"
        app:layout_constraintTop_toTopOf="parent" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar2"
        app:progress="1" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar3"
        app:progress="0.75" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar7"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar4"
        app:progress="0.5" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar8"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar5"
        app:progress="0.25" />

    <kanti.fillingprogressbar.FillingProgressBar
        android:id="@+id/fillingProgressBar10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        app:layout_constraintStart_toEndOf="@+id/fillingProgressBar9"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar6" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/checkBox"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar7" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true"
        android:enabled="false"
        app:layout_constraintStart_toEndOf="@+id/checkBox2"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar8" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        app:layout_constraintStart_toEndOf="@+id/checkBox3"
        app:layout_constraintTop_toBottomOf="@+id/fillingProgressBar9" />

</androidx.constraintlayout.widget.ConstraintLayout>